{
  "name": "usertoken",
  "version": "0.0.1",
  "description": "Usertoken standard environment.",
  "private": true,
  "main": "app.js",
  "scripts": {
    "clean": "npx shx rm -rf ./build",
    "prebuild": "npm run clean;npx shx mkdir -p ./build",
    "build": "npx npm-run-all -l -p build:*",
    "build:js": "npx shx cp -r *.js ./build",
    "build:json": "npx shx cp -r *.json ./build",
    "build:yaml": "npx shx cp -r *.yaml ./build",
    "build:assets": "npx shx cp -r assets ./build",
    "build:lib": "npx shx cp -r lib ./build",
    "build:ui": "npx shx cp -r ui ./build",
    "start": "node app.js",
    "dev": "nodemon --watch \"./\" --exec \"node app.js\"",
    "predeploy": "gcloud debug source gen-repo-info-file",
    "deploy": "gcloud app deploy",
    "test": "mocha test/*.test.js --exit --timeout=20000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleCloudPlatform/nodejs-docs-samples.git"
  },
  "engines": {
    "node": ">=14"
  },
  "author": "nautilus.",
  "license": "Private",
  "dependencies": {
    "@lasso/marko-taglib": "^1.0.15",
    "@marko/compiler": "^5.10.2",
    "@marko/express": "^1.0.0",
    "@usertoken/memory": "0.0.1",
    "browser-refresh-taglib": "^1.1.0",
    "chai": "^4.2.0",
    "diffhtml": "^1.0.0-beta.10",
    "express": "4.17.1",
    "gun": "0.2020.301",
    "hyperswarm": "2.15.3",
    "ignoring-watcher": "^1.0.5",
    "lasso": "^3.4.0",
    "lasso-marko": "^2.4.7",
    "nanomorph": "^5.4.0",
    "peer": "0.6.1",
    "quick-lru": "5.1.1",
    "shelljs": "0.8.4",
    "sodium-plus": "^0.9.0",
    "url": "0.11.0",
    "vdom-virtualize": "2.0.0",
    "virtual-dom": "^2.1.1",
    "winston": "^3.3.3",
    "ws": "7.4.6"
  },
  "devDependencies": {
    "jshint": "^2.7.0",
    "mocha": "8.0.0",
    "mocha-chrome": "^2.2.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "path": "0.12.7",
    "rollup": "^1.4.1",
    "shx": "0.3.3",
    "uglify-js": "^3.7.0"
  }
}
